filter(method == "Trait-based dissimilarity") %>%
group_by(method, group, subject, tbin, data) %>%
summarise(dist = mean(dist)) %>%
group_by(method, group, tbin) %>%
do(mod = t.test(dist ~ data, .)) %>%
mutate(pval = mod$p.value,
sig = pval < 0.1,
symb = ifelse(pval < 0.001, '***',
ifelse(pval < 0.01, '**',
ifelse(pval < 0.05, '*',
ifelse(pval < 0.1, '', '')))))
